[sub_resource type="GDScript" id="GDScript_88p4v"]
script/source = "extends VBoxContainer

signal add_component(component:String)
signal layer_edited(layer_data:Dictionary)
signal footer_collapsed(is_collapsed:bool)

var contents : Dictionary

func _ready() -> void:
	var idx : int = 0
	for each in $TabContainer.get_children():
		contents[each.name] = idx
		if idx == %tabs.current_tab:
			$TabContainer.current_tab = idx
		idx += 1
	
	%style_name.text = \"Gold Over Blue\"
	%layer_styles.add_item(\"Gold Over Blue\")
	%layer_styles.select(0)

func _exit_tree() -> void:
	G.setts.set_value(\"0\", \"tray_selected\", %tabs.current_tab)

func read_settings():
	var selected_tab = G.setts.get_value(\"0\", \"tray_selected\", %tabs.current_tab)
	$TabContainer.current_tab = selected_tab
	%tabs.current_tab = selected_tab

func _on_tabs_tab_selected(tab: int) -> void:
	var tab_name = %tabs.get_tab_title(tab)
	$TabContainer.current_tab = contents[tab_name]
	%display_tray.button_pressed = true

func _on_display_tray_toggled(toggled_on: bool) -> void:
	%display_tray.text = [\"Show Tray\", \"Hide Tray\"][int(toggled_on)]
	$TabContainer.visible = toggled_on
	size_flags_vertical = [SIZE_SHRINK_END, SIZE_EXPAND_FILL][int(toggled_on)]
	footer_collapsed.emit(toggled_on)


func _on_component_pressed(component:String) -> void:
	add_component.emit(component)

#region Handle Current Layer.
func set_layer_data(title:String):
	%layer_title.text = title

func _on_layer_title_text_submitted(new_text: String) -> void:
	if new_text.is_empty():
		new_text = \"A Layer\"
		%layer_title.text = new_text
	var data = {\"title\": new_text}
	layer_edited.emit(data)
	
#endregion


func _on_edit_style_toggled(toggled_on: bool) -> void:
	%style_name.visible = toggled_on
	%layer_styles.visible = not toggled_on
"

